<div class="full-height">
	<div class="col-md-4 col-md-offset-4" id="auth-left">
		
		<div class="middle">
			<div class="row">
				
			</div>
			<form id="form-login">
				<p class="login-box-msg">Sign in to start your session</p>
				<div class="row">
					<input name="login[username]" placeholder="USERNAME" type="text" class="form-control"/>
				</div>
				<div class="row">
					<input name="login[password]" placeholder="PASSWORD" type="password" class="form-control"/>
				</div>
				<div class="row text-left">
					<input name="login[html5]" placeholder="HTML5" type="checkbox" value=-1 onclick="value=0-value" class=""/>
					Use HTML5 Only
				</div>
				<div class="row">
					<div class="col-xs-4 col-xs-offset-8">
						<input alt="Login Icon" class="btn btn-primary btn-block btn-flat" type="submit" value="Sign In"/>
					</div>
				</div>
			</form>
		</div>
	</div>
	
</div> <!-- --> 
